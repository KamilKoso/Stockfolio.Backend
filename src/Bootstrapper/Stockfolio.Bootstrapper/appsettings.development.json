{
  "logging": {
    "logLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "logger": {
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": false
    },
    "tags": {}
  },
  "vault": {
    "enabled": false
  },
  "redis": {
    "connectionString": "localhost:6379"
  }
}